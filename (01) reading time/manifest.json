{
  "name": "Reading time",
  "description": "Add the reading time to Chrome Extension documentation articles",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["script/content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
